#include <iostream>  
using namespace std;  
class Employee {  
   public:  
       int id;    
       string name;    
       float salary;  
       void insert(int i, string n, float s) {    
            id = i;    
            name = n;    
            salary = s;  
        }    
       void display() {    
            cout << id << "  " << name << "  " << salary << endl;    
        }    
};  
int main() {  
    Employee e1, e2, e3;  
    e1.insert(101, "sonu", 85000);    
    e2.insert(102, "monu", 45000);    
    e3.insert(103, "Chai", 67000);    
    e1.display();    
    e2.display();    
    e3.display();    
    return 0;  
}  
